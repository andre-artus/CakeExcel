{
    "name": "andre-artus/cakeexcel",
    "type": "cakephp-plugin",
    "description": "CakePHP excel generator",
    "keywords": [
        "cakephp",
        "dakota",
        "excel",
        "phpexcel"
    ],
    "homepage": "https://github.com/andre-artus/cakeexcel",
    "license": "MIT",
    "authors": [
        {
            "name": "Walther Lalk",
            "email": "emailme@waltherlalk.com"
        }
    ],
    "modifier": [
        {
            "name": "Daren Sipes",
            "email": "daren@sipes.me"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^4.0|^5.0",
        "phpoffice/phpspreadsheet": ">=1.2",
        "phpunit/phpunit": "^8.0|^10.*"
    },
    "autoload": {
        "psr-4": {
            "CakeExcel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeExcel\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "support": {
        "issues": "https://github.com/dakota/CakeExcel/issues",
        "source": "https://github.com/dakota/CakeExcel"
    }
}
